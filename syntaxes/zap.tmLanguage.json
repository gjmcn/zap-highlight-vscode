{
	"name": "Zap",
	"patterns": [
		{ "include": "#comments" },
		{ "include": "#numbers" },
		{ "include": "#strings-single" },
		{ "include": "#strings-double" },
		{ "include": "#regexps" },
		{ "include": "#constants" },
		{ "include": "#variables" },
		{ "include": "#keywords-word" },
		{ "include": "#identifiers" },
		{ "include": "#punctuation" },
		{ "include": "#keywords-symbols" }
	],
	"repository": {
		"comments": {
			"patterns": [{
				"name": "comment.zap",
				"match": "\/\/.*"
			}]
		},
		"numbers": {
			"patterns": [{
				"name": "constant.numeric.zap",
				"match": "0[bB][01]+n?|0[oO][0-7]+n?|0[xX][\\da-fA-F]+n?|0n|[1-9]\\d*n|(?:\\.\\d+|\\d+(?:\\.\\d*)?)(?:e[+\\-]?\\d+)?"
			}]
		},
		"strings-single": {
			"patterns": [{
				"name": "string.zap",
				"match": "'[^'\\\\]*(?:\\\\.[^'\\\\]*)*'"
			}]
		},
		"strings-double": {
			"name": "string.zap",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.zap",
					"match": "\\\\."
				}
			]
		},
		"regexps": {
			"patterns": [{
				"name": "string.regexp.zap",
				"match": "&\\/(?!\\/)[^\\/\\\\]*(?:\\\\.[^\\/\\\\]*)*\\/[\\w$]*"
			}]
		},
		"constants": {
			"patterns": [{
				"name": "constant.language.zap",
				"match": "(?:true|false|null|undefined|Infinity|NaN)(?![\\w$])"
			}]
		},
		"variables": {
			"patterns": [{
				"name": "variable.language.zap",
				"match": "(?:super|this)(?![\\w$])"
			}]
		},
		"keywords-word": {
			"patterns": [{
				"name": "keyword.zap",
				"match": "\\$(?:new|await(?:Each|Map|Try)?|ms|void|delete|typeof|instanceof|in|yieldFrom|yield|if|each|map|async(?:Do|While|Fun|Proc|Gen)|do|while|fun|proc|gen|nest(?:Each|Map)?|zip(?:Each|Map)?|seq|to|linSpace|throw|try|debugger|print|reduce|some|every|filter|minIndex|min|maxIndex|max|sumCumu|sum|count|findIndex|find|orderIndex|order|mean|groupCount|group|binCount|bin|variance|deviation|arrObj|objArr|transpose|class|extends|assign|attach|pick|into|insertEach|insert|encodeSVG|encode|createSVG|create|fragment|attr|prop|style|text|html|remove|lower|raise|addClass|removeClass|hasClass|hasAttr|removeAttr|removeStyle|on|off|sketch|abs|acos|acosh|asin|asinh|atan|atanh|cbrt|ceil|clz32|cos|cosh|exp|expm1|floor|fround|log|log10|log1p|log2|round|sign|sin|sinh|sqrt|tan|tanh|trunc|toUpperCase|toLowerCase|trim|trimEnd|trimStart|neg|boolean|number|string|date|isInteger|isFinite|isNaN|not|array|at|ats|get|set|random|randomInt|categorical|normal|logNormal|binomial|exponential|geometric|shuffle|export|import|importAs|importDefault|importAll|a|abbr|address|area|article|aside|audio|b|bdi|bdo|blockquote|br|button|canvas|caption|cite|code|col|colgroup|datalist|dd|del|details|dfn|dialog|div|dl|dt|em|embed|fieldset|figcaption|figure|footer|form|h1|h2|h3|h4|h5|h6|header|hgroup|hr|i|iframe|img|input|ins|kbd|label|legend|li|link|main|mark|menu|meta|meter|nav|noscript|ol|optgroup|option|output|p|picture|pre|progress|q|rb|rp|rt|rtc|ruby|s|samp|script|section|select|slot|small|source|span|strong|sub|summary|sup|table|tbody|td|template|textarea|tfoot|th|thead|time|title|tr|track|u|ul|var|video|wbr|animate|animateMotion|animateTransform|circle|clipPath|defs|desc|discard|ellipse|feBlend|feColorMatrix|feComponentTransfer|feComposite|feConvolveMatrix|feDiffuseLighting|feDisplacementMap|feDistantLight|feDropShadow|feFlood|feFuncA|feFuncB|feFuncG|feFuncR|feGaussianBlur|feImage|feMerge|feMergeNode|feMorphology|feOffset|fePointLight|feSpecularLighting|feSpotLight|feTile|feTurbulence|foreignObject|g|hatch|hatchpath|image|line|linearGradient|marker|mask|metadata|mpath|path|pattern|polygon|polyline|radialGradient|rect|solidcolor|stop|svg|symbol|textPath|tspan|use|view)(?![\\w$])"
			}]
		},
		"identifiers": {
			"patterns": [{
				"name": "variable.zap",
				"match": "[a-zA-Z_$][\\w$]*"
			}]
		},
		"punctuation": {
			"patterns": [{
				"name": "punctuation.zap",
				"match": "\\??[\\[{]|\\((?:[\\-=]>)?|[)}\\];,]|\\.{3}"
			}]
		},
		"keywords-symbols": {
			"patterns": [{
				"name": "keyword.zap",
				"match": "`?(?:[+\\-*/%\\^?\\\\=@#<>!]?=|[+\\-*/%\\^\\\\!]|<\\\\|\\|\\||&&|<>?|><|>|@{1,2}|#{1,2}|\\?{1,2}|[<?:]?:)`?(?![+\\-*%<>=!?\\\\#@:\\|\\^`]|\\/(?:$|[^/])|&&)"
			}]
		}
	},
	"scopeName": "source.zap"
}